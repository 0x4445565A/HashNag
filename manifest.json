{
  "manifest_version": 2,
  "name": "Hashnag",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*",
        "https://www.instagram.com/*",
        "https://twitter.com/*"
      ],
      "js": ["jquery-2.1.4.min.js", "hashnag.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
